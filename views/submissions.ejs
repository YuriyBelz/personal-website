<div class="container">
  <div class="panel-wrapper">
    <div class="left-colum">
      <div class="panel">
        <div class="title">
          <h1>All Submissions, newest appear first</h1>
        </div>
        <div class="content">
          <p>click on an entry to see more information</p>
          <div >
            <ul>
              <%allSubmissions.forEach(function(post){%>
                    <a href=<%="/submission/" + post._id%>><div class="subSmall" id=<%=post._id%>>
                      <h3 class="subSmallTitle"><%=post.title%></h3><br><br>
                      <p class ="subsmallInfo">By: <%=post.owner%><br>
                      <%=post.description%><br>
                      Created: <%=post.submitcreated%></p>
                      </div></a>
              <% }); %>
            </ul>
          </div>
        </div>
      </div>
    </div>
    </div>
