<%- include('partials/headerpartial.ejs') %>
<!--- menu-wrapper div end -->
<div class="clearing"></div>
<div class="border-bottom"></div>
<div class="logo-wrapper">
  <!--<div class="leftshadow"><img src="images/logo-wrap-left.jpg" /></div>-->
  <div class="logo">
    <h1>YuriyBelz.com</h1>
  </div>
 <!--<div class="rightshadow"><img src="images/logo-wrap-right.jpg" /></div> -->
</div>
<div class="clearing"></div>
<!--- banner wrapper div end -->
<div class="container">
  <div class="panel-wrapper">
    <div class="left-colum">
      <div class="panel">
        <div class="title">
          <h1><%=allSubmissions.length %> Submissions found, newest appear first</h1>
        </div>
        <div class="content">
          <p>click on an entry to see more information</p> 
          
          <div >
            <ul>
              <% for(var i = 0; i < allSubmissions.length; i++){ %>
                    
                    <a href=<%= "/submission/" + allSubmissions[i]._id %>><div class="subSmall" id=<%=allSubmissions[i]._id %>>
                      <h3 class="subSmallTitle"><%= allSubmissions[i].title %></h3><br><br>
                      <p class ="subsmallInfo">By: <%= allSubmissions[i].owner %><br>
                      <%= allSubmissions[i].description.substring(0, 30) + ". . ." %><br>
                      Created: <%= allSubmissions[i].submitcreated %></p>
                      </div></a>
              <% } %>
            </ul>
          </div>
        </div>
      </div>
    </div>
    </div>
    <div class="clearing"></div>
  </div>
  <!--- panel wrapper div end -->
<!--- container div end -->
<div class="clearing"></div>

